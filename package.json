{
  "name": "florianfarke.com",
  "version": "1.0.0",
  "description": "Personal website of Florian M. Farke.",
  "keywords": [
    "about me",
    "education",
    "contact"
  ],
  "private": "true",
  "targets": {
    "dev": {
      "source": "src/index.html",
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    },
    "prod": {
      "source": "src/index.html",
      "distDir": "./public/"
    }
  },
  "scripts": {
    "serve": "parcel serve --target dev",
    "build": "parcel build --target prod",
    "clear": "rm -rf './dist'",
    "lint": "eslint",
    "check": "tsc --noEmit"
  },
  "author": "Florian Farke",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-typescript-tsc": "^2.7.0",
    "@types/bootstrap": "^5.2.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "parcel": "^2.7.0",
    "posthtml-include": "^1.7.4",
    "typescript": "^4.8.2"
  }
}
