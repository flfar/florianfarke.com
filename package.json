{
  "name": "florianfarke.com",
  "version": "1.0.0",
  "description": "Personal website of Florian M. Farke.",
  "keywords": [
    "about me",
    "education",
    "contact"
  ],
  "private": "true",
  "targets": {
    "dev": {
      "source": "src/index.html",
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    },
    "prod": {
      "source": "src/index.html",
      "distDir": "./dist/"
    }
  },
  "scripts": {
    "serve": "parcel serve --target dev",
    "build": "parcel build --target prod",
    "clear": "rm -rf './dist'",
    "lint": "eslint",
    "check": "tsc --noEmit"
  },
  "author": "Florian Farke",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.2.0-beta1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.6.0",
    "@parcel/transformer-typescript-tsc": "^2.6.0",
    "@types/bootstrap": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "parcel": "^2.6.0",
    "typescript": "^4.7.3"
  }
}
